            color: white;
        }

        .section {
            padding: 80px 10%;
        }

        .title {
            opacity: 0;
            transform: translateY(50px);
            transition: opacity 1s ease, transform 1s ease;
            text-align: center;
        }

            .title h1 {
                font-size: 3.5em;
            }

        .show {
            opacity: 1;
            transform: translateY(0);
        }

        .cycles {
            font-size: 2.5em;
            cursor: pointer;
            transition: font-size 0.3s ease;
            margin-bottom: 60px;
            text-align: left;
        }

            .cycles:hover {
                font-size: 3em;
            }

        .cycle-details {
            display: none;
            padding-left: 20px;
            margin-top: 20px;
        }

            .cycle-details ul {
                list-style-type: none;
                padding: 0;
            }

            .cycle-details li {
                margin-bottom: 25px;
            }

        .year {
            font-size: 1.7em;
            cursor: pointer;
            margin-bottom: 10px;
        }

            .year:hover {
                font-size: 1.9em;
            }

        .semester {
            cursor: pointer;
            font-size: 1.4em;
            margin: 8px 0 8px 20px;
            padding-left: 10px;
        }

        .subjects {
            display: none;
            margin-left: 40px;
            font-size: 1.1em;
            margin-top: 20px;
        }

        .subject-card {
            background-color: #2a3b4d;
            border-radius: 10px;
            padding: 20px;
            margin-bottom: 20px;
            position: relative;
        }

        .subject-title {
            font-size: 1.4em;
            font-weight: bold;
        }

        .subject-info {
            font-size: 0.95em;
            color: #cbd6e2;
            margin-top: 8px;
        }

        .ects-wrapper {
            position: absolute;
            top: 45px;
            right: 20px;
            text-align: center;
        }

        .ects-circle {
            background-color: #5dade2;
            color: #fff;
            width: 50px;
            height: 50px;
            border-radius: 50%;
            line-height: 50px;
            font-size: 1.2em;
            margin: 0;
        }

        .ects-label {
            font-size: 0.75em;
            margin-top: 4px;
            color: #d0e4f5;
        }
    </style>
</head>
